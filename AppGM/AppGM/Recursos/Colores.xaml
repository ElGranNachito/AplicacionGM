    <system:SolidColorBrush PresentationOptions:Freeze="true" x:Key="Color.TabItem.Borde.Deshabilitado" Color="Gray"/>

    <system:SolidColorBrush PresentationOptions:Freeze="true" x:Key="Color.TabItem.Texto.Estatico" Color="{StaticResource Tema.Color.VerdeOscuro}"/>
