    <system:SolidColorBrush PresentationOptions:Freeze="true" x:Key="Color.CheckBox.Fondo.MouseOver" Color="{StaticResource Tema.Color.VerdeSemiOscuro}"/>
    <system:SolidColorBrush PresentationOptions:Freeze="true" x:Key="Color.CheckBox.Borde.MouseOver" Color="{StaticResource Tema.Color.Verde}"/>
    <system:SolidColorBrush PresentationOptions:Freeze="true" x:Key="Color.CheckBox.Tilde.MouseOver" Color="{StaticResource Tema.Color.Verde}"/>

