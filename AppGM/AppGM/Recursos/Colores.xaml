    <system:SolidColorBrush PresentationOptions:Freeze="true" x:Key="Color.TabItem.Fondo.MouseOver" Color="{StaticResource Tema.Color.Verde}"/>
    <system:SolidColorBrush PresentationOptions:Freeze="true" x:Key="Color.TabItem.Fondo.Deshabilitado" Color=" Gray"/>

